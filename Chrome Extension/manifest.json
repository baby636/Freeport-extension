{
   "author": "Joe Looney",
   "description": "Collect, Trade and Create Cryptogoods",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "48": "images/icon-48.png"
   },
   "background": {
      "scripts": ["js/background.js"],
      "persistent": false
   },
   "browser_action": {
      "default_icon": "images/icon-48.png"
   },
   "web_accessible_resources": [
       "js/*.*",
       "images/*.*",
       "css/*.*",
       "html/*.*"
   ],
   "content_scripts": [{
     "matches": ["https://*.digirare.com/*"],
     "js": ["js/lib/jquery-3.4.1.min.js", "js/contentscript.js"],
     "all_frames": true
   },
   {
     "matches": ["https://*.bandcamp.com/*"],
     "js": ["js/lib/jquery-3.4.1.min.js", "js/contentscript-bc.js"],
     "all_frames": true
   }],
   "permissions": [
        "storage",
        "https://*.digirare.com/",
        "https://*.xchain.io/",
        "activeTab"
   ],
   "manifest_version": 2,
   "name": "Freeport",
   "short_name": "freeport",
   "version": "0.3.0"
}
